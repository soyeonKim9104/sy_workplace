ght:2px;\r\n      background:#fff;\r\n      z-index:10;\r\n    }\r\n    &::before{\r\n      transform: rotate(0deg) translate(0px,-5px);\r\n    }\r\n    &::after {\r\n      transform: rotate(0deg) translate(0px, 5px);\r\n    }\r\n  }\r\n  &.active .bar::before{\r\n    transform: rotate(-45deg) translate(0px,0px);\r\n  }\r\n  &.active .bar::after{\r\n    transform: rotate(45deg) translate(0px,0px);\r\n  }\r\n}\r\n\r\n.sideBar{\r\n  position: fixed;\r\n  background:$ham-bg;\r\n  left: 0;\r\n  top: 0;\r\n  width:4.2vw;\r\n  height:100vh;\r\n  z-index:3;\r\n}\r\n\r\n#cate{\r\n  position: fixed;\r\n  background:$ham-bg;\r\n  top:0;\r\n  left:0;\r\n  width:420px;\r\n  height:100vh;\r\n  padding: 5vw 0 0 5vw;\r\n  margin: 0;\r\n  transform: translate(-100%, 0);\r\n  z-index: 9;\r\n  h2{\r\n    color:#fff;\r\n    font-size: 2.5rem;\r\n  }\r\n  ul{\r\n      margin-top:4vh;\r\n    li{\r\n      list-style:none;\r\n      a{\r\n        text-decoration: none;\r\n        display: block;\r\n        color: $text-default;\r\n        font-size: 1.5rem;\r\n        font-weight: 500;\r\n      }\r\n    }\r\n    li + li{\r\n      margin-top:2vh;\r\n    }\r\n  }\r\n  &.active {\r\n    transform: translate(0%, 0);\r\n  }\r\n}\r\n\r\n.main{\r\n  padding: 8vh 10vw;\r\n}\r\n\r\n.mainHead{\r\n  .logo{\r\n    color:$text-default;\r\n    font-size: 2.7rem;\r\n  }\r\n  .txt{\r\n    color:$text-default;\r\n    font-family: 'Kanit', sans-serif;\r\n    font-size:2.3rem;\r\n    font-weight:500;\r\n    margin-top: 4vh;\r\n  }\r\n}\r\n\r\n.contents{\r\n  .title{\r\n    color:$text-default;\r\n    border:1px solid $text-default;\r\n    font-size: 1.3rem;\r\n    margin-top: 4vh;\r\n    width: auto;\r\n    height: 5vh;\r\n    line-height: 4.5vh;\r\n    padding: 0 2vw;\r\n    display: inline-block;\r\n    border-radius: 2vw;\r\n  }\r\n  .work{\r\n    margin-top: 4vh;\r\n    display:flex;\r\n    .box{\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n      text-align: center;\r\n      width: 20%;\r\n      height: 25vh;\r\n      background: rgb(39,44,48);\r\n      background: linear-gradient(132deg, rgba(39,44,48,1) 0%, rgba(28,31,34,1) 100%);\r\n      border-radius: 1.5vw;\r\n      text-decoration: none;\r\n      color:$text-default;\r\n      font-size: 1.2rem;\r\n    }\r\n    .box + .box{\r\n      margin-left:2vw;\r\n    }\r\n  }\r\n}\r\n\r\n.footAni{\r\n  position: fixed;\r\n  left: 0;\r\n  bottom: 0;\r\n  width: 100%;\r\n  height: 3vw;\r\n  line-height: 3vw;\r\n  z-index: 2;\r\n  background:$ham-bg;\r\n  white-space: nowrap;\r\n  .license{\r\n    width: 100%;\r\n    display:inline-block;\r\n    font-family: 'Anton', sans-serif;\r\n    color:red;\r\n    text-align:center;\r\n    font-size:1.5rem;\r\n    animation: marAni 10s linear infinite;\r\n  }\r\n  @keyframes marAni {\r\n    100% {\r\n      transform: translateX(-100%);\r\n    }\r\n  }\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"0b988cb16a7542a5a6b5800d7e5711d1","cacheData":{"env":{}}}